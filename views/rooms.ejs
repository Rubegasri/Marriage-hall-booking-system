<!-- rooms.ejs -->
<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Rooms</title>
  <!-- Add any additional stylesheets or scripts if needed -->
</head>
<body>
  <h1>Rooms</h1>
  <!-- Display rooms data -->
  <div id="rooms-list">
    <% rooms.forEach(room => { %>
      <div class="room-item">
        <h2><%= room.name %></h2>
        <p>Capacity: <%= room.capacity %></p>
        <!-- Display additional details as needed -->
      </div>
    <% }); %>
  </div>
</body>
</html>
